#!/bin/sh
# $poplocal/local/rclib/mkrcautolinks
#   Aaron Sloman   11 Jul 1997
# Create links for autoloadable procedures

cd auto

ln -s ../lib/rc_button_utils.p create_action_button.p
ln -s ../lib/rc_button_utils.p create_blob_button.p
ln -s ../lib/rc_button_utils.p create_option_button.p
ln -s ../lib/rc_button_utils.p create_select_button.p
ln -s ../lib/rc_button_utils.p create_radio_button_columns.p
ln -s ../lib/rc_button_utils.p create_someof_button_columns.p
ln -s ../lib/rc_button_utils.p create_button_row.p
ln -s ../lib/rc_button_utils.p create_button_column.p

echo "links done"
