# Do not edit this file by hand, it will be overwritten by configure
# make.in as the template

# standard autoconf/configure variables

CC = @CC@
LN_S = @LN_S@
RANLIB = @RANLIB@
OBJEXT = @OBJEXT@
DEFS = @DEFS@
CFLAGS = @CFLAGS@ 

LIBS = @LIBS@
HAVE_LIBX = @HAVE_LIBX@
HAVE_LIBXEXT = @HAVE_LIBXEXT@
HAVE_LIBX11 = @HAVE_LIBX11@
HAVE_LIBXM = @HAVE_LIBXM@

# Poplog variables derived from the ones above
POP__cc = $(CC)

POP_LINK_ARGS = -nox

# POP_SETARCH = 'setarch i386 -R'
POP_SETARCH = 

# uname -m 
POP_X86 = i686
# uname -o lower case, punctuation converted to _
POP_OS = gnu_linux
# uname -r
POP_LINUX_KERNEL = 2.6.32-305-ec2
POP_LINUX_KERNEL_VERSION = 2.6

